{
  "extends": ["next/core-web-vitals", "next", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true
  },
  "rules": {
    "no-unused-vars": [
      1,
      {
        "args": "after-used"
      }
    ]
  }
}
